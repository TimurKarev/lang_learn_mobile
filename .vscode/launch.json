    "version": "0.2.0",
    "configurations": [
        {
            "name": "dev-analytics",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--dart-define=FLAVOR=dev"
            ]
        },
        {
            "name": "dev",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--dart-define=FLAVOR=dev_no_analytics"
            ]
        },
        {
            "name": "release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--flavor=prod",
                "--dart-define=FLAVOR=prod"
            ]
        },
        {
           "name": "profile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--flavor=prod",
                "--dart-define=FLAVOR=prod"
            ]
        }
    ]
}